{
  "name": "discogs-tempo-adjust",
  "version": "0.1",
  "description": "A browser extension to detect and adjust track tempo on Discogs",
  "author": "Bobby Azarbayejani <bobby@azarbayejani.com>",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tempo-adjust/wxt-hooks": "workspace:*",
    "playwright": "^1.57.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.6.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "wxt": "^0.20.13"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt --browser firefox --mv3",
    "build": "wxt build",
    "build:firefox": "wxt build --browser firefox --mv3",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip --browser firefox",
    "postinstall": "wxt prepare",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@tempo-adjust/permissions": "workspace:*",
    "@tempo-adjust/player-components": "workspace:*",
    "@tempo-adjust/theme-provider": "workspace:*",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@wxt-dev/module-react": "^1.1.5",
    "@wxt-dev/webextension-polyfill": "^1.0.0",
    "classnames": "^2.5.1",
    "i": "^0.3.7",
    "pnpm": "^10.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-overlay": "^6.0.11",
    "react-query": "^3.39.3",
    "webextension-polyfill": "^0.10.0",
    "zustand": "^5.0.0-rc.2"
  },
  "type": "module",
  "packageManager": "pnpm@9.12.3"
}
